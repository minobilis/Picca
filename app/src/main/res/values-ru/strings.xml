    <string name="Page1Message">The world\'s <![CDATA[<b>fastest</b>]]> messaging app.<![CDATA[<br/>]]>It is <![CDATA[<b>free</b>]]> and <![CDATA[<b>secure</b>]]>.</string>
    <string name="Page2Message"><![CDATA[<b>Picca</b>]]> delivers messages faster than<![CDATA[<br/>]]>any other application.</string>
    <string name="Page3Message"><![CDATA[<b>Picca</b>]]> is free forever. No ads.<![CDATA[<br/>]]>No subscription fees.</string>
    <string name="Page4Message"><![CDATA[<b>Picca</b>]]> keeps your messages safe<![CDATA[<br/>]]>from hacker attacks.</string>
    <string name="Page5Message"><![CDATA[<b>Picca</b>]]> has no limits on the size of<![CDATA[<br/>]]>your media and chats.</string>
    <string name="Page6Message"><![CDATA[<b>Picca</b>]]> lets you access your messages<![CDATA[<br/>]]>from multiple devices.</string>
    <string name="Page7Message"><![CDATA[<b>Picca</b>]]> messages are heavily encrypted<![CDATA[<br/>]]>and can self-destruct.</string>
